			"settings": {
				"foreground": "#7296B3"
			}
		},
		{
      "name": "Warn Tokens",
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#D9C671"
			}
		},
		{
      "name": "Error Tokens",
			"scope": "token.error-token",
			"settings": {
				"foreground": "#E35339"
			}
		},
		{
