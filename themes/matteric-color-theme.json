	"tokenColors": [
		{
      "name": "Punctuation, Storage, Keywords",
			"scope": [
				"constant.character.escape",
				"punctuation",
        "meta.brace",
				"storage.type",
        "storage.modifier",
				"keyword.control",
        "keyword.operator"
			],
			"settings": {
				"foreground": "#4D535C"
			}
		},
    {
      "name": "Entity Types",
      "scope": "support.type",
      "settings": {
        "foreground": "#ACAEB0"
      }
    },
    {
      "name": "At-Rules",
      "scope": "keyword.control.at-rule",
      "settings": {
        "foreground": "#ACAEB0"
      }
    },
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#D9C671"
			}
		},
    {
			"name": "Strings, Regular Expressions",
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#84A360"
			}
		},
    {
      "name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#E38539"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#E35339"
			}
		},
		{
			"name": "HTML Tags",
			"scope": "entity.name.tag.html",
			"settings": {
				"foreground": "#6F91AD"
			}
		},
    {
      "name": "Info Tokens",
