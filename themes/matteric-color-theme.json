	"tokenColors": [
		{
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
        "meta.diff.header.from-file",
        "meta.diff.header.to-file",
				"meta.separator",
			],
			"settings": {
				"foreground": "#ACAEB0"
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"entity.name",
				"variable.other"
			],
			"settings": {
				"foreground": "#ACAEB0",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#6AAA51"
			}
		},
		{
			"scope": [
				"constant",
				"constant.numeric",
				"constant.keyword",
				"constant.character"
			],
			"settings": {
				"foreground": "#CC7B45"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#C5B052",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#D14848"
			}
		},
		{
			"scope": [
				"keyword.control.export",
				"keyword.control.default",
				"keyword.control.flow"
			],
			"settings": {
				"foreground": "#6CA5E6CC"
			}
		},
		{
			"scope": [
				"constant.character.escape",
				"punctuation",
        "meta.brace.square",
