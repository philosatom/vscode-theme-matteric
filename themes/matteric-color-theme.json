        "storage.modifier",
        "storage.control",
				"keyword.operator",
				"keyword.control",
			],
			"settings": {
				"foreground": "#566370"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6CA5E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#C5B052"
			}
		},
		{
			"scope": "token.error-token",
